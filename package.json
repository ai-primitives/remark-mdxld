{
  "name": "remark-mdxld",
  "version": "0.1.1",
  "description": "Remark plugin for MDX with integrated support for YAML and YAML-LD Frontmatter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ai-primitives/mdx.org.ai.git"
  },
  "homepage": "https://mdxld.org",
  "bugs": {
    "url": "https://github.com/ai-primitives/mdx.org.ai/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint src",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write ."
  },
  "keywords": [
    "remark",
    "mdx",
    "yaml-ld",
    "frontmatter",
    "unified"
  ],
  "author": "AI Primitives",
  "license": "MIT",
  "dependencies": {
    "remark": "^15.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-mdx": "^3.1.0",
    "remark-stringify": "^11.0.0",
    "unified": "^11.0.5",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/mdast": "^4.0.4",
    "@types/node": "^20.10.0",
    "@types/unist": "^3.0.3",
    "@types/vfile": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "remark-parse": "^11.0.0",
    "semantic-release": "^24.2.0",
    "typescript": "^5.3.0",
    "vfile": "^6.0.3",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@mdx-js/mdx": "^3.0.0"
  }
}
